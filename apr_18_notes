#range

for no in range(5):
    print(no, end=' ')
print()
for no in range(1,5):
    print(no, end=' ')
print()
for no in range(1,10,2):
    print(no, end=' ')
print()
for no in range(5,1,-1):
    print(no, end=' ')
print()
for no in range(10,3,-2):
    print(no, end=' ')
