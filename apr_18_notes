name = "education"
vowels="aeiou"
count_vowels =0
for charact in name:
    if charact in vowels:
        count_vowels +=1
else:
    print("Vowels count from the string is ",count_vowels)

name = "education"
#vowels="aeiou"
count_vowels =0
for charact in name:
    if charact in "aeiou":
        count_vowels +=1
else:
    print("Vowels count from the string is ",count_vowels)
