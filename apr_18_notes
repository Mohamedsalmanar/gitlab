#removing space at front
sentence = '             how are you'
for letter in sentence: 
    if letter == ' ':
        pass
    else:
        print(letter,end='')
-----------
#removing space at front
sentence = '             how are you'
first_letter = False
for letter in sentence: 
    if letter == ' ' and first_letter == False:
        continue
    else:
        first_letter = True
        print(letter, end='')
-----------
#removing space at the end
sentence = 'how are you         '
last_letter = False
l = len(sentence)
i = l - 1
while i>=0:
    if sentence[i]==' ' and last_letter==False:
        i-=1
        continue
    else:
        last_letter = True
        print(sentence[i],end='')
    i-=1
    
----------------
#removing space at the end
sentence = 'how are you         '
last_letter = False
l = len(sentence)
i = l - 1
while i>=0:
    if sentence[i]!=' ':
        end = i
        start = 0
        while start<=end:
            print(sentence[start],end='')
            start+=1
        break 
    i-=1   
---------------
