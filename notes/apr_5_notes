def add(a, b):
    return a + b

result = add(5, 3)
print("Sum:", result)  # Output: Sum: 8


public class ElectricityBilling {

    public static int ebReading() {
        int units = 120;
        return units;
    }

    public static void calculateAmount(int unit) {
        if (unit < 100) {
            System.out.println("Free");
        } else if (unit < 200) {
            System.out.println(unit * 1.20);
        } else if (unit < 300) {
            System.out.println(unit * 2);
        }
    }
}


